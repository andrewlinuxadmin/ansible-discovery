{
  "name": "grafana-json-tree-panel",
  "version": "1.0.0",
  "description": "Grafana panel plugin for displaying JSON data in an interactive tree view",
  "main": "dist/module.js",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "build-no-lint": "grafana-toolkit plugin:build --skipLint",
    "build-webpack": "webpack --mode=production",
    "dev": "grafana-toolkit plugin:dev",
    "test": "grafana-toolkit plugin:test",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "repository": {
    "type": "git",
