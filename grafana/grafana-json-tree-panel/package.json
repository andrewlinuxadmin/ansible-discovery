    "url": "https://github.com/andrewlinuxadmin/ansible-discovery/issues"
  },
  "devDependencies": {
    "@grafana/data": "^9.5.0",
    "@grafana/toolkit": "^9.5.0",
    "@grafana/ui": "^9.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.9.0",
    "@emotion/css": "^11.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "ts-loader": "^9.0.0",
    "style-loader": "^3.0.0",
    "css-loader": "^6.0.0"
  },
  "peerDependencies": {
    "@grafana/data": "^9.0.0",
    "@grafana/ui": "^9.0.0",
